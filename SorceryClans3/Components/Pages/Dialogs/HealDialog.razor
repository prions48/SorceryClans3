<MudDialog>
    <DialogContent>
        <HealInspector CompleteMission="Close" MedicTeam="MedicTeam" PatientTeam="PatientTeam" Settings="Settings" />
    </DialogContent>
</MudDialog>

@code{
    [Parameter, EditorRequired] public Team MedicTeam { get; set; } = default!;
    [Parameter, EditorRequired] public Team PatientTeam { get; set; } = default!;
    [Parameter, EditorRequired] public GameSettings Settings { get; set; } = default!;
    [CascadingParameter] MudDialogInstance MudDialog { get; set; } = default!;
    private void Close()
    {
        MudDialog.Close(DialogResult.Cancel());
    }
}